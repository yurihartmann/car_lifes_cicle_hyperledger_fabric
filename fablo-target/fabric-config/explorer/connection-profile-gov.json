{
  "name": "fablo-test-network-gov",
  "description": "Connection profile for Hyperledger Explorer in Fablo network",
  "version": "1.0.0",
  "client": {
    "organization": "Gov",
    "tlsEnable": false,
    "enableAuthentication": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "Gov": {
      "mspid": "GovMSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/gov.car-lifes-cicle.com/users/Admin@gov.car-lifes-cicle.com/msp/keystore/priv-key.pem"
      },
      "peers": [
        "peer0.gov.car-lifes-cicle.com",
        "peer1.gov.car-lifes-cicle.com"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/gov.car-lifes-cicle.com/users/Admin@gov.car-lifes-cicle.com/msp/signcerts/Admin@gov.car-lifes-cicle.com-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.gov.car-lifes-cicle.com": {
      "url": "grpc://peer0.gov.car-lifes-cicle.com:7021",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/gov.car-lifes-cicle.com/msp/tlscacerts/tlsca.gov.car-lifes-cicle.com-cert.pem"
      }
    },
    "peer1.gov.car-lifes-cicle.com": {
      "url": "grpc://peer1.gov.car-lifes-cicle.com:7022",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/gov.car-lifes-cicle.com/msp/tlscacerts/tlsca.gov.car-lifes-cicle.com-cert.pem"
      }
    }
  },
  "channels": {
    "car-lifes-cicle-channel": {
      "peers": {}
    }
  }
}
