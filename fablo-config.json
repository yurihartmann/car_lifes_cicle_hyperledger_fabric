      },
      "peer": {
        "instances": 2,
        "db": "LevelDb"
      },
      "tools": {}
    },
    {
      "organization": {
        "name": "mecanicaA",
        "domain": "mecanica-a.car-lifes-cicle.com"
      },
      "peer": {
        "instances": 2,
        "db": "LevelDb"
      },
      "tools": {}
    },
    {
      "organization": {
        "name": "seguradoraA",
