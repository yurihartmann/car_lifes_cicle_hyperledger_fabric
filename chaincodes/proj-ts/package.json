{
  "name": "doxa-chaincode",
  "version": "1.0.0",
  "description": "Doxa hyperledger chaincode",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --fix -c tslint.json './src/**/*.ts'",
    "test": "npm run lint && mocha -r ts-node/register tests/**/*.spec.ts --reporter spec",
    "test:w": "npm run lint && mocha -r ts-node/register tests/**/*.spec.ts --reporter spec --watch app/**/*.spec.ts --watch-extensions ts",
    "clean": "rm -rf dist",
    "start": "node dist/index.js"
  },
  "author": "Doxa",
  "license": "MIT",
  "dependencies": {
    "@theledger/fabric-chaincode-utils": "^4.0.3",
    "@types/lodash": "^4.14.136",
    "fabric-shim": "^1.4.2",
    "lodash": "^4.17.15",
    "winston": "^3.2.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@theledger/fabric-mock-stub": "^4.0.0",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.5.8",
    "@types/winston": "^2.3.8",
    "@types/yup": "^0.24.1",
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "nodemon": "^1.12.1",
    "ts-node": "^5.0.1",
    "tslint": "4.5.1",
    "typescript": "^2.6.2"
  },
  "engines": {
    "node": "^8.9",
    "npm": "^5.6"
  }
}